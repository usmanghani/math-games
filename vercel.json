{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm install && pnpm --filter number-line-adventure build",
  "devCommand": "pnpm --filter number-line-adventure dev",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "games/number-line-adventure/.next",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/games/number-line-adventure/$1"
    }
  ]
}
